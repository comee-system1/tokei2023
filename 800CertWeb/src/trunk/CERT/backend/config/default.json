            "error_logger_appender": {
                "type": "dateFile",
                "filename": "log/error.log",
                "backups": 14,
                "compress": true,
                "pattern": "-yyyy-MM-dd",
                "layout": {
                    "type": "pattern",
                    "pattern": "[%d{yyyy-MM-dd hh:mm:ss.SSS}][%5p][%X{requestId}] %m"
                }
            },
            "error_logger": {
                "type": "logLevelFilter",
                "appender": "error_logger_appender",
                "level": "error"
            },
            "out": {
                "type": "stdout",
                "layout": {
                    "type": "pattern",
                    "pattern": "[%d{yyyy-MM-dd hh:mm:ss.SSS}][%5p][%X{requestId}] %m"
                }
            }
        },
        "categories": {
            "default": {
                "appenders": [
                    "logger",
                    "error_logger"
                ],
                "level": "debug",
                "enableCallStack": true
            }
        }
    }
