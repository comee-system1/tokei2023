    "log4js": {
        "appenders": {
            "logger": {
                "type": "dateFile",
                "filename": "log/app.log",
                "backups": 14,
                "compress": true,
                "pattern": "-yyyy-MM-dd",
                "layout": {
                    "type": "pattern",
